<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="1.6.0" client="site" method="upgrade">
	<name>mod_phocagallery_slideshow_floom</name>
	<author>Jan Pavelka (www.phoca.cz)</author>
	<creationDate>07/04/2011</creationDate>
	<copyright>Jan Pavelka</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail></authorEmail>
	<authorUrl>www.phoca.cz</authorUrl>
	<version>1.0.3</version>
	<description>MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DESCRIPTION</description>
	
	<files>
		<folder>css</folder>
		<folder>images</folder>
		<folder>javascript</folder>
		<folder>tmpl</folder>
		<folder>language</folder>
		<filename module="mod_phocagallery_slideshow_floom">mod_phocagallery_slideshow_floom.php</filename>
		<filename>index.html</filename>
	</files>
	<languages>
		<language tag="en-GB">language/en-GB/en-GB.mod_phocagallery_slideshow_floom.ini</language>
		<language tag="en-GB">language/en-GB/en-GB.mod_phocagallery_slideshow_floom.sys.ini</language>
	</languages>

	
	<config>
		<fields name="params">
			<fieldset name="basic">

<field name="category_id" type="text" default="" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_CATEGORY_ID_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_CATEGORY_ID_DESC" />
<field name="count_images" type="text" default="5" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_COUNT_IMAGES_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_COUNT_IMAGES_DESC" />

<field name="width" type="text" default="970" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_WIDTH_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_WIDTH_DESC" />
<field name="height" type="text" default="230" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_HEIGHT_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_HEIGHT_DESC" />
<field name="duration" type="text" default="70" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DURATION_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DURATION_DESC" />
<field name="interval" type="text" default="8000" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_INTERVAL_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_INTERVAL_DESC" />

<field name="display_progressbar" type="list" default="1" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DISPLAY_PROGRESSBAR_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DISPLAY_PROGRESSBAR_DESC">
	<option value="0">MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_NO</option>
	<option value="1">MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_YES</option>
</field>

<field name="display_desc" type="list" default="1" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DISPLAY_DESC_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_DISPLAY_DESC_DESC">
	<option value="0">MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_NO</option>
	<option value="1">MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_YES</option>
</field>

<field name="css_description" type="textarea" rows="15" cols="40" default="width:500px;
	height:60px;
	padding: 10px;
	-moz-border-radius: 10px 0px 0px 0px;
	-webkit-border-radius: 10px 0px 0px 0px;
	border-radius: 10px 0px 0px 0px;
	color: #000;
	bottom:0px;
	right:0px;
	background: #fff;
	position:absolute;" label="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_CSS_DESC_LABEL" description="MOD_PHOCAGALLERY_SLIDESHOW_FLOOM_CSS_DESC_DESC" />
	
	
	
			</fieldset>
			<fieldset name="advanced">
		
<field
					name="moduleclass_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				
				
				
				<field
					name="cache"
					type="list"
					default="0"
					label="COM_MODULES_FIELD_CACHING_LABEL"
					description="COM_MODULES_FIELD_CACHING_DESC">
					<option
						value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
				</field>

		
			</fieldset>
		</fields>
	</config>
</extension>
